<% include ../partials/header %>
<div class="container">
<h1>Product Detail</h1>
<div class="form-group">
    <label for="">ID</label>
    <input type="text" value="<%= game.id %>" readonly
      class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
    <small id="helpId" class="form-text text-muted">Help text</small>
</div>

<div class="form-group">
    <label for="">Type</label>
    <input type="text" value="<%= game.type %>" readonly
      class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
    <small id="helpId" class="form-text text-muted">Help text</small>
</div>

<div class="form-group">
    <label for="">name</label>
    <input type="text" value="<%= game.name %>" readonly
      class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
    <small id="helpId" class="form-text text-muted">Help text</small>
</div>

<div class="form-group">
    <label for="">Price</label>
    <input type="text" value="<%=game.price %>" readonly
      class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
    <small id="helpId" class="form-text text-muted">Help text</small>
</div>


<p class="lead">
    <a class="btn btn-primary btn-lg" href="/product" role="button"><< Back to Product List</a>
</p>
</div>
<% include ../partials/footer %>


